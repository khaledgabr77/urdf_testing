<?xml version="1.0"?>
<robot name="four_wheel_robot" xmlns:xacro="http://www.ros.org/wiki/xacro">
    <xacro:include filename="$(find urdf_testing)/urdf/robot.gazebo" />

    <link name="base_link" />
    <link name="chassis">
        <inertial>
            <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0" />
            <mass value="5.0" />
            <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="1.0" iyz="0.0" izz="1.0" />
        </inertial>
        <visual name="">
            <origin xyz="0.0 0.0 0.1" rpy="0.0 0.0 0.0" />
            <geometry>
                <box size="0.6 0.4 0.1" />
            </geometry>

        </visual>
        <collision>
            <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0" />
            <geometry>
                <box size="0.6 0.4 0.1" />
            </geometry>
        </collision>
    </link>
    <joint name="base_joint" type="fixed">
        <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0" />
        <parent link="base_link" />
        <child link="chassis" />
    </joint>
    <link name="right_wheel">
        <inertial>
            <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0" />
            <mass value="1.0" />
            <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="1.0" iyz="0.0" izz="1.0" />
        </inertial>
        <visual name="">
            <origin xyz="0.2 0.4 0.0" rpy="1.5705 0.0 0.0" />
            <geometry>
                <cylinder radius="0.1" length="0.05" />
            </geometry>

        </visual>
        <collision>
            <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0" />
            <geometry>
                <cylinder radius="0.1" length="0.05" />
            </geometry>
        </collision>
    </link>
    <joint name="right_wheel_joint" type="continuous">
        <origin xyz="0.0 -0.2 0.03" rpy="0.0 0.0 0.0" />
        <parent link="chassis" />
        <child link="right_wheel" />
        <axis xyz="0.0 0.0 0.0" />
    </joint>


    <link name="left_wheel">
        <inertial>
            <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0" />
            <mass value="1.0" />
            <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="1.0" iyz="0.0" izz="1.0" />
        </inertial>
        <visual name="">
            <origin xyz="0.2 -0.4 0.0" rpy="1.5705 0.0 0.0" />
            <geometry>
                <cylinder radius="0.1" length="0.05" />
            </geometry>

        </visual>
        <collision>
            <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0" />
            <geometry>
                <cylinder radius="0.1" length="0.05" />
            </geometry>
        </collision>
    </link>
    <joint name="left_wheel_joint" type="continuous">
        <origin xyz="0.0 0.2 0.03" rpy="0.0 0.0 0.0" />
        <parent link="chassis" />
        <child link="left_wheel" />
        <axis xyz="0.0 0.0 0.0" />
    </joint>
